{
	"$schema": "https://biomejs.dev/schemas/1.9.4/source.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": false
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"correctness": {
				"noUnusedVariables": "warn"
			},
			"style": {
				"noDefaultExport": "off",
				"useImportType": "warn"
			}
		}
	}
}
